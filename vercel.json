{
    "version": 2,
    "builds": [
      {
        "src": "app.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "headers": [
      {
        "source": "pubic/css/(.*)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/css"
          }
        ]
      }
    ]
  }

 
  